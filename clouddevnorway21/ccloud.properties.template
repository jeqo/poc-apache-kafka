sasl.mechanism=PLAIN
sasl.jaas.config=org.apache.kafka.common.security.plain.PlainLoginModule required username="{{ CCLOUD_KAFKA_API_KEY }}" password="{{ CCLOUD_KAFKA_API_SECRET }}";
